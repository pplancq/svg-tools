{
  "name": "@pplancq/svg-react",
  "version": "2.1.2",
  "license": "MIT",
  "description": "A React component designed to load SVG files efficiently",
  "author": "pplancq <paul.plancq@outlook.fr>",
  "main": "./build/Svg.js",
  "types": "./build/Svg.d.ts",
  "exports": {
    ".": {
      "import": "./build/Svg.js",
      "types": "./build/Svg.d.ts"
    },
    "./tests": {
      "import": "./build/helper/tests/index.js",
      "types": "./build/helper/tests/index.d.ts"
    }
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pplancq/svg-tools.git",
    "directory": "packages/svg-react"
  },
  "homepage": "https://pplancq.github.io/svg-tools/",
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "concurrently --prefix-colors auto \"npm:eslint\" \"npm:tsc\"",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "tsc": "tsc --noEmit"
  },
  "engines": {
    "node": ">= 20"
  },
  "bugs": {
    "url": "https://github.com/pplancq/svg-tools/issues"
  },
  "keywords": [
    "svg",
    "react"
  ],
  "dependencies": {
    "@pplancq/svg-core": "2.0.0"
  },
  "peerDependencies": {
    "@testing-library/react": "^16.3.0",
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "@testing-library/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@pplancq/eslint-config": "^5.0.5",
    "@pplancq/prettier-config": "^1.2.7",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rslib/core": "^0.13.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.12",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.4"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "prettier": "@pplancq/prettier-config",
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --fix",
    "*.{ts,tsx}": "tsc-files --noEmit"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
